from ua_gec import Corpus
import stanza

corpus = Corpus(partition="test")
# stanza.download('uk')
nlp = stanza.Pipeline('uk')
doc = nlp("Я хочу їв.")


print(*[f'word: {word.text+" "}\tlemma: {word.lemma}' for sent in doc.sentences for word in sent.words], sep='\n')

